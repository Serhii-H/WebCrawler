{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=WebCrawlerDB;Integrated Security=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "WebCrawler": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "ApplicationSettings": {
    "MediaTypeToCrawl": [
      "text/plain",
      "text/html"
    ],
    "HrefsToIgnore": [
      ".js",
      ".css",
      ".jpg",
      ".jpeg",
      ".png",
      ".bmp",
      ".svg",
      ".ico",
      ".pdf"
    ]
  },
  "AllowedHosts": "*"
}
